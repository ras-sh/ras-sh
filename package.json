    "clsx": "^2.1.1",
    "convex": "^1.27.3",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "onnxruntime-web": "1.21.0-dev.20250206-d981b153d3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
